{
  "name": "apollo-server",
  "version": "0.1.1",
  "description": "Apollo GraphQL server for Node.js",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha -- --reporter spec --full-trace test/index.js",
    "lint": "eslint .",
    "testonly": "mocha test/index.js",
    "prepublish": "babel ./src --ignore test --out-dir ./dist",
    "postinstall": "npm dedupe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollostack/apollo-server.git"
  },
  "keywords": [
    "GraphQL",
    "Apollo",
    "Meteor",
    "Proxy",
    "Cache",
    "Javascript"
  ],
  "author": "Jonas Helfer <jonas@helfer.email>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apollostack/apollo-proxy/issues"
  },
  "homepage": "https://github.com/apollostack/apollo-proxy#readme",
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "express-graphql": "^0.5.1",
    "graphql-tools": "^0.3.10"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "6.3.21",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.0",
    "chai": "^3.5.0",
    "dataloader": "^1.1.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.1.0",
    "express": "^4.13.4",
    "express3": "0.0.0",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^2.3.3",
    "multer": "^1.0.3",
    "nodemon": "^1.9.1",
    "request-promise": "^2.0.1",
    "supertest": "^1.0.1",
    "supertest-as-promised": "^2.0.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
